{
    "name": "openpublicmedia/pbs-tv-schedules-service-php",
    "description": "PHP client for consuming the PBS TV Schedules Service (TVSS) API.",
    "type": "library",
    "license": "Apache-2.0",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3 || ^7.0"
    },
    "require-dev": {
        "phpcompatibility/php-compatibility": "dev-develop",
        "phpunit/phpunit": "^8.2",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "OpenPublicMedia\\PbsTvSchedulesService\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenPublicMedia\\PbsTvSchedulesService\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "platform-check": false,
        "platform": {
            "php": "8.0"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
